                    <dd id='cancle_server' tid='' title="点击取消 游戏服：" onclick="cancleSelect($(this))"><a click="#"><span><em></em></span></a></dd>
                    <dd id='cancle_race' title="点击取消 阵营：" onclick="cancleSelect($(this))"><a click="#"><span><em></em></span></a></dd>
                </dl>
            </div>
        </div>
