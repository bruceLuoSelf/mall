/*
 * 区服交易流水信息
 */
Ext.define('MyApp.model.gameTradingModel', {
    extend: 'Ext.data.Model',
    idProperty: 'extId',
    idgen: 'uuid',
    fields: [{
        name: 'gameName'//游戏名称
    },{
        name: 'region'//所在区
    },{
        name: 'server'//所在服
    },{
        name: 'tradingNum'//成交笔数
    },{
        name: 'total',//订单总额
        type: 'float'
    },{
        name: 'commission',//佣金收入)
        type: 'float'
    },{
    	name: 'difference',//差额收入
    	type: 'float'
    },{
        name: 'goldCount',//购买总数(一个区服购买金币数)
        type: 'int'
    }]
});