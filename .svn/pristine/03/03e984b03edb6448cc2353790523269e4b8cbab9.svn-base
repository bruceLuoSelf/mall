package com.wzitech.gamegold.rc8.service.servicer.dto;

import com.wzitech.chaos.framework.server.common.AbstractServiceRequest;

/**
 * @author yemq
 *
 */
public class LoginRequest extends AbstractServiceRequest {

    /**
     * 客服账号
     */
    private String name;

    /**
     * 客服密码--des加密
     */
    private String pwd;

    /**
     * 移动密保
     */
    private String movepwd;

    /**
     * 签名
     */
    private String sign;

    /**
     * RC客户端版本
     */
    private String version;

    public LoginRequest() {}

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getPwd() {
        return pwd;
    }

    public void setPwd(String pwd) {
        this.pwd = pwd;
    }

    public String getMovepwd() {
        return movepwd;
    }

    public void setMovepwd(String movepwd) {
        this.movepwd = movepwd;
    }

    public String getSign() {
        return sign;
    }

    public void setSign(String sign) {
        this.sign = sign;
    }

    public String getVersion() {
        return version;
    }

    public void setVersion(String version) {
        this.version = version;
    }
}
