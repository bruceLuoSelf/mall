package com.wzitech.gamegold.mymgmt.entity;

import java.math.BigDecimal;

import com.wzitech.chaos.framework.server.dataaccess.BaseEntity;

/**
 * 省份
 * @author HeJian
 *
 */
public class ProvinceEO extends BaseEntity {
	private static final long serialVersionUID = 1L;

	/**
	 * 省份名称
	 */
	private String proName;
	
	/**
	 * 运费
	 */
	private BigDecimal fare;
	
	/**
	 * 省份排名
	 */
	private int proSort;
	
	/**
	 * 省份说明
	 */
	private String proRemark;

	/**
	 * @return the proName
	 */
	public String getProName() {
		return proName;
	}

	/**
	 * @param proName the proName to set
	 */
	public void setProName(String proName) {
		this.proName = proName;
	}

	/**
	 * @return the proSort
	 */
	public int getProSort() {
		return proSort;
	}

	/**
	 * @param proSort the proSort to set
	 */
	public void setProSort(int proSort) {
		this.proSort = proSort;
	}

	/**
	 * @return the proRemark
	 */
	public String getProRemark() {
		return proRemark;
	}

	/**
	 * @param proRemark the proRemark to set
	 */
	public void setProRemark(String proRemark) {
		this.proRemark = proRemark;
	}

	public BigDecimal getFare() {
		return fare;
	}

	public void setFare(BigDecimal fare) {
		this.fare = fare;
	}
	
}
