body {width:800px; margin-left: auto; margin-right: auto;font-family: 微软雅黑;}
.content{border:1px solid #cdc9c9;padding: 15px;}
#header {width:760px; height:40px; background-color:white;color:#434345;}
#queryForm {width:780px; height:30px;margin: 20px auto;margin-left: 55px;}
#mainBody {width:750px; height:475px;padding: 10px;}
#mainBody table tr.title td{
    height: 30px;font-size:15px;color:#434345;}
#mainBody table tr td{text-align: center;}
#tile {width:769px; height:50px; color:red;line-height: 50px;vertical-align: middle;font-size:16px;text-align: left;font-weight: bold;padding-left:15px;}
.query {float:left; width:110px;margin-right: 15px;}
.query #btnSearch{width:100px;height: 25px;}
td {width:144px;font-size:12px;height: 28px;color:#666666;}
.thin {width:60px;}
.tr1 {background-color:#f6f5f5;}
.tr0 {background-color:white;}
#tile {align:center;}
select{width:110px;height: 30px;line-height:30px;vertical-align: middle;}