package com.wzitech.gamegold.goods.entity;

import com.wzitech.chaos.framework.server.dataaccess.BaseEntity;

import java.util.Date;

/**
 * 前台安心买热卖商品配置
 *
 * @author yemq
 */
public class HotRecommendConfig extends BaseEntity {
    /**
     * 关联的游戏ID
     */
    private Long gameId;

    /**
     * 关联的游戏名称
     */
    private String gameName;

    /**
     * 游戏币购买数量下拉菜单数值，多个单价以逗号分隔
     */
    private String goldCounts;

    /**
     * 产品栏目1图标，多个以逗号分隔
     */
    private String categoryIcon1;
    /**
     * 产品栏目2图标，多个以逗号分隔
     */
    private String categoryIcon2;
    /**
     * 产品栏目3图标，多个以逗号分隔
     */
    private String categoryIcon3;
    /**
     * 是否显示栏目1的最低价商品
     */
    private boolean isShowCategory23;

    /**
     * 寄售模式
     */
    private Boolean isConsignmentMode;

    /**
     * 寄售模式开始时间
     */
    private String consignmentStartTime;

    /**
     * 寄售模式结束时间
     */
    private String consignmentEndTime;

    /**
     * 操作者ID
     */
    private Long operatorId;

    /**
     * 创建时间
     */
    private Date createTime;

    /**
     * 最后更新时间
     */
    private Date lastUpdateTime;

    /**
     * 通货类目ID
     * wrf 5.11新增
     */
    private Long goodsTypeId;

    /**
     * 通货类目名称
     * wrf 5.11新增
     */
    private String goodsTypeName;

    public Long getGoodsTypeId() {
        return goodsTypeId;
    }

    public void setGoodsTypeId(Long goodsTypeId) {
        this.goodsTypeId = goodsTypeId;
    }

    public String getGoodsTypeName() {
        return goodsTypeName;
    }

    public void setGoodsTypeName(String goodsTypeName) {
        this.goodsTypeName = goodsTypeName;
    }

    public HotRecommendConfig() {
    }

    public Long getGameId() {
        return gameId;
    }

    public void setGameId(Long gameId) {
        this.gameId = gameId;
    }

    public String getGameName() {
        return gameName;
    }

    public void setGameName(String gameName) {
        this.gameName = gameName;
    }

    public String getGoldCounts() {
        return goldCounts;
    }

    public void setGoldCounts(String goldCounts) {
        this.goldCounts = goldCounts;
    }

    public String getCategoryIcon1() {
        return categoryIcon1;
    }

    public void setCategoryIcon1(String categoryIcon1) {
        this.categoryIcon1 = categoryIcon1;
    }

    public String getCategoryIcon2() {
        return categoryIcon2;
    }

    public void setCategoryIcon2(String categoryIcon2) {
        this.categoryIcon2 = categoryIcon2;
    }

    public String getCategoryIcon3() {
        return categoryIcon3;
    }

    public void setCategoryIcon3(String categoryIcon3) {
        this.categoryIcon3 = categoryIcon3;
    }

    public Long getOperatorId() {
        return operatorId;
    }

    public void setOperatorId(Long operatorId) {
        this.operatorId = operatorId;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Date getLastUpdateTime() {
        return lastUpdateTime;
    }

    public void setLastUpdateTime(Date lastUpdateTime) {
        this.lastUpdateTime = lastUpdateTime;
    }

    public boolean isShowCategory23() {
        return isShowCategory23;
    }

    public void setShowCategory23(boolean isShowCategory23) {
        this.isShowCategory23 = isShowCategory23;
    }

    public Boolean getIsConsignmentMode() {
        return isConsignmentMode;
    }

    public void setIsConsignmentMode(Boolean isConsignmentMode) {
        this.isConsignmentMode = isConsignmentMode;
    }

    public String getConsignmentStartTime() {
        return consignmentStartTime;
    }

    public void setConsignmentStartTime(String consignmentStartTime) {
        this.consignmentStartTime = consignmentStartTime;
    }

    public String getConsignmentEndTime() {
        return consignmentEndTime;
    }

    public void setConsignmentEndTime(String consignmentEndTime) {
        this.consignmentEndTime = consignmentEndTime;
    }
}
