}

.form-checkbox, .form-radio {
    float: none;
    display: inline-block;
    overflow: hidden;
    position: relative;
    margin-right: 15px;
    cursor: pointer;
    vertical-align: middle;
    color: #666;
}

.form-checkbox:hover, .form-radio:hover {
    text-decoration: none;
}

.form-checkbox .icon {
    margin: 0 2px -3px 0;
}

.icon-checkbox {
    width: 16px;
    height: 16px;
    background-position: -15px -17px;
}

.icon {
    display: inline-block;
}

.form-checkbox input, .form-radio input {
    position: absolute;
    left: -999em;
}

.form-checkbox-checked .icon-checkbox {
    background-position: -41px -17px;
}

