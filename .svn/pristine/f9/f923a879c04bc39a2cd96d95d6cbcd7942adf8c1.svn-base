.filter .btn-refresh {
    position: absolute;
    right: 10px;
    top: 10px;
    width: 34px;
    height: 67px;
    border: 1px solid #CECED0;
    background: #FFE2B1;
    background: -webkit-linear-gradient(#FEFEFE, #F7F7F7);
    background: -moz-linear-gradient(#FEFEFE, #F7F7F7);
    background: linear-gradient(#FEFEFE, #F7F7F7);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#FFFEFEFE, endColorstr=#FFF7F7F7);
    text-align: center;
    line-height: 1.5;
    overflow: hidden;
}

.filter .btn-refresh:hover {
    background: #FFE2B1;
    background: -webkit-linear-gradient(#F7F7F7, #FEFEFE);
    background: -moz-linear-gradient(#F7F7F7, #FEFEFE);
