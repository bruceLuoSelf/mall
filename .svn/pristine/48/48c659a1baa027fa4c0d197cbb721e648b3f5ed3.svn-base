.new_rest{
	border:none !important;
	height:auto !important;
}
.new_rest .tbody{
	border: 1px solid #ececec;
    border-top: none;
}
.new_rest .thead{
	border:1px solid #ececec;
}
.new_rest .new-table{
	clear: both;
    margin-top: 20px;
    text-align: center;
}
.new_rest .new-table thead td{
	border: 1px solid #ddd;
    line-height: 40px;
	font-family:'宋体';
}
.new_rest .new-table tbody td{
	border: 1px solid #ddd;
    line-height: 40px;
	font-family:'宋体';
	height:70px;
}

.new_rest .new_item em{
	display:block;
	line-height: 32px;
	font-size:12px;
	color:#999;
}
.new_rest .new_item a{
	color:#ccc;
	display:inline-block;
	width:55px;
	height:26px;
	line-height:26px;
	border-radius:5px;
}
.new_rest .new_item a.elect_order{
	background-color:#3983e8;
	color:#fff;
	margin-right:10px;
}
.new_rest .new_item a.ote_order{
	background-color:#ff9a00;
	color:#fff;
}
.new_rest .myskin{
	background-color:#f5f5f5;
	border:1px solid #e8e8e8;
	height:70px;
	vertical-align:middle;
}
.new_rest .myskin td{
	border:none !important;
}
.new_rest .myskin a {
	width:55px;
	height:26px;
	line-height:26px;
	text-align:center;
	display:inline-block;
	border-radius:5px;
}
.new_rest .myskin a.slt_order{
	background-color:#3983e8;
	color:#fff;
}
.new_rest .myskin a.slt_num{
	background-color:#ff9a00;
	color:#fff;
	margin-left:12px;
}
.new_rest  a.active{
	background-color:#ddd !important;
	color:#bbb !important;
}
/* 申请补单结果页面 */
.underdefid{
	width:950px;
	height:20px;
}
.slet_out{
	width:950px;
	margin:0 auto;
	border:1px solid #ddd;
}
.slet_out .succ_neworder{
	padding:20px 50px;
}
.slet_out .succ_neworder .title_neworder{
	height:70px;
	line-height:70px;
	border-bottom:1px dashed #ddd;

}
.slet_out .succ_neworder .title_neworder span{
	display:inline-block;
	width:31px;
	height:31px;
	float:left;
	margin-top:20px;
}
.slet_out .succ_neworder .title_neworder .succ_newrec{
	background: url("../img/succ_order.png") center no-repeat;
}
.slet_out .succ_neworder .title_neworder .midd_newrec{
	background: url("../img/new_error.png") center no-repeat;
}
.slet_out .succ_neworder .title_neworder .failed_newrec{
	background: url("../img/failed_order.png") center no-repeat;
}
.slet_out .succ_neworder .title_neworder  em{
	float:left;
	margin-left:15px;
	font-size:14px;
	font-weight:bold;
}
.slet_out .succ_neworder .title_neworder .rc-tag-qq{
	float:right;
	margin-top:10px;
}
.slet_out .detail_neworder{
	padding:0px 50px;
}
.slet_out .detail_neworder .rectop{
	text-align:center;
	height:100px;
	line-height:100px;
	font-size:14px;
	font-weight:bold;
}
.slet_out .detail_neworder  dl{
	font-size:12px;
}
.slet_out .detail_neworder  dl dt{
	display: block;
    width: 48%;
    height: 35px;
    line-height: 35px;
    text-align: right;
    float: left;
}
.slet_out .detail_neworder  dl dd{
	display: block;
    height: 35px;
    line-height: 35px;
    width: 50%;
    float: right;
}
.slet_out .cloud_new .xx{
	border:none;
}
