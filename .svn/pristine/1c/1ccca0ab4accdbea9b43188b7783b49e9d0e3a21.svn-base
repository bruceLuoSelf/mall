.rc-fp-cont input{height:27px;border:1px solid #ddd;padding-left:5px}
.rc-fp-cont select{height:30px;border:1px solid #ddd;margin-right:10px;margin-top:-1px}
.rc-fp-but{padding-left:99px;}
.margin-right10{margin-right:10px}
.poprm{position:fixed;top:0;left:0;display:none;width:100%;height:100%;z-index:99999}
.poprmCont{position:relative;top:300px;background:#fff;height:150px;margin:auto;width:300px;filter:alpha(opacity=100);z-index:99}
.poprmContClose{float:right;margin-right:10px;font-size:12px;font-weight:normal;cursor:pointer}
.poprmContTag{height:35px;background:#ddd;line-height:35px; padding-left:20px;font-size:14px;font-weight:bold}
.poprmContNun{clear:both}
.poprmContNun input{height:30px;line-height:30px;margin:20px;width:80%;padding:0 10px;}
.poprmContBut{text-align:right;padding-right:18px}
.poprmBg{position:relative;top:-150px;background:#000;filter:alpha(opacity=50);opacity:0.5;width:100%;height:100%;}
.clear{clear:both}

