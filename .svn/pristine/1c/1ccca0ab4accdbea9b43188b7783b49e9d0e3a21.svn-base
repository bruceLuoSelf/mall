/*收货订单详情*/
.rc-tag{padding:30px 0 10px 0;font-size:16px;font-weight:bold;border-bottom:1px dashed #D5E7FE;}
.rc-tag-qq{float:right}
.rc-text{border:1px solid #D5E7FE;background:#f6faff;padding:20px 35px;margin-top:20px}
.rc-text-l{width:300px;float:left;line-height:35px;}
.rc-table{clear:both;margin-top:20px;text-align:center;}
.rc-table td{border:1px solid #ddd;line-height:40px;}
.rc-cancel,.rc-success{padding:0;background:#f1f1f1;border:1px solid #dddddd;width:90px;line-height:25px;margin-right:10px;color:#666;font-weight:normal; text-shadow:none;float:none}
.rc-success{background:#ff9a00;color:#fff;border-color:#d07e01;margin-right:0px;}
.rc-fp{margin-top:30px;}
.rc-fp-tag{font-size:14px;font-weight:bold;}
