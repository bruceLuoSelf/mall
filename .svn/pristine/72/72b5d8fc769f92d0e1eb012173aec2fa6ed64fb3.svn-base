	<!-- 查询订单信息 -->
	<select id="selectOrder" parameterType="String" resultMap="com.wzitech.gamegold.order.entity.OrderInfoEO.BaseResultMap">  
        SELECT * FROM "TBL_GAMEGOLD_ORDERS" WHERE "ORDER_ID" = #{id}
    </select>
    
    <!-- 查询库存信息 -->
	<select id="selectRepository" parameterType="long" resultMap="com.wzitech.gamegold.repository.entity.RepositoryInfo.BaseResultMap">  
        <!-- SELECT * FROM "TBL_GAMEGOLD_REPOSITORY" WHERE "ID" = #{id} AND "IS_DELETED" = false -->
        <!-- 库存被删除，这里isdeleted=false，就会查询不到这个库存，即查询不到卖家库存账号信息，有异常 -->
        SELECT * FROM "TBL_GAMEGOLD_REPOSITORY" WHERE "ID" = #{id}
    </select>
    
     <!-- 查询客服信息 -->
